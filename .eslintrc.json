{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": "off",
        "no-undef" :"off",
        "max-len" : ["error", {"code": 210 }],
        "no-plusplus" :"off",
        "no-console" :"off"
    }
}
